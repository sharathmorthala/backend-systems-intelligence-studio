# Backend Systems Intelligence Studio - Environment Variables
# Copy this file to .env and fill in your values

# =============================================================================
# REQUIRED
# =============================================================================

# Hugging Face API Key (for AI-powered analysis)
# Get your free API token at: https://huggingface.co/settings/tokens
# Without this key, the app will use deterministic fallback analysis
HUGGINGFACE_API_KEY=hf_your_api_token_here

# =============================================================================
# OPTIONAL
# =============================================================================

# Contact form email recipient
# Default: morthalasharath@gmail.com
CONTACT_EMAIL=your-email@example.com

# Server port (default: 5000)
PORT=5000

# Node environment (development | production)
NODE_ENV=development
